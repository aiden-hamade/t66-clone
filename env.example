# T66 Environment Configuration

# ========================================
# APPLICATION SETTINGS
# ========================================
APP_NAME=T66
APP_URL=http://localhost:3000
NODE_ENV=development

# ========================================
# POCKETBASE SETTINGS
# ========================================
POCKETBASE_URL=http://localhost:8090
POCKETBASE_ADMIN_EMAIL=admin@t66.dev
POCKETBASE_ADMIN_PASSWORD=changeme123

# ========================================
# AI PROVIDER API KEYS
# ========================================

# OpenAI
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_ORG_ID=org-your-org-id-here

# Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Google (Gemini/PaLM)
GOOGLE_AI_API_KEY=your-google-ai-key-here
GOOGLE_PROJECT_ID=your-google-project-id

# Cohere
COHERE_API_KEY=your-cohere-key-here

# OpenRouter (Multi-provider)
OPENROUTER_API_KEY=sk-or-your-openrouter-key-here

# Hugging Face
HUGGINGFACE_API_KEY=hf_your-huggingface-key-here

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# ========================================
# IMAGE GENERATION PROVIDERS
# ========================================

# DALL-E (via OpenAI)
DALLE_API_KEY=$OPENAI_API_KEY

# Midjourney (via unofficial API)
MIDJOURNEY_API_KEY=your-midjourney-key-here

# Stable Diffusion
STABILITY_API_KEY=sk-your-stability-key-here

# ========================================
# EXTERNAL SERVICES
# ========================================

# Web Search
SERP_API_KEY=your-serpapi-key-here
BING_SEARCH_API_KEY=your-bing-search-key-here

# YouTube Data API
YOUTUBE_API_KEY=your-youtube-api-key-here

# File Storage (S3 compatible)
S3_ENDPOINT=https://s3.amazonaws.com
S3_BUCKET=t66-uploads
S3_ACCESS_KEY=your-s3-access-key
S3_SECRET_KEY=your-s3-secret-key
S3_REGION=us-east-1

# ========================================
# SECURITY & AUTHENTICATION
# ========================================

# JWT Settings
JWT_SECRET=your-super-secret-jwt-key-here-change-this
JWT_EXPIRES_IN=7d

# OAuth Providers
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret

# ========================================
# DEVELOPMENT & DEBUGGING
# ========================================

# Enable debug mode
DEBUG=true

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Enable request logging
LOG_REQUESTS=true

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# ========================================
# FEATURE FLAGS
# ========================================

# Enable experimental features
ENABLE_VOICE_MODE=false
ENABLE_COMPUTER_USE=false
ENABLE_CODE_EXECUTION=false
ENABLE_DEEP_RESEARCH=true
ENABLE_MEMORY_SYSTEM=true

# ========================================
# ANALYTICS & MONITORING
# ========================================

# Analytics
ANALYTICS_ENABLED=false
ANALYTICS_API_KEY=your-analytics-key-here

# Sentry (Error tracking)
SENTRY_DSN=your-sentry-dsn-here
SENTRY_ENVIRONMENT=development

# ========================================
# CONTENT MODERATION
# ========================================

# OpenAI Moderation
MODERATION_ENABLED=true
MODERATION_API_KEY=$OPENAI_API_KEY

# Custom moderation service
CUSTOM_MODERATION_ENDPOINT=https://your-moderation-service.com/api

# ========================================
# LIMITS & QUOTAS
# ========================================

# Token limits per user (per day)
MAX_TOKENS_PER_USER_DAY=100000

# File upload limits
MAX_FILE_SIZE_MB=50
MAX_FILES_PER_UPLOAD=10

# Chat limits
MAX_CHATS_PER_USER=100
MAX_MESSAGES_PER_CHAT=1000

# Image generation limits
MAX_IMAGES_PER_USER_DAY=50 